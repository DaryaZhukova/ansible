[Unit]
Description=Jenkins war file running script
After=syslog.target network.target remote-fs.target nss-lookup.target

[Service]

Type=simple
User={{ user_name }}
ExecStart=/usr/bin/java -DJENKINS_HOME=/home/{{ user_name }}/ -jar /home/{{ user_name }}/jenkins.war &


[Install]
WantedBy=multi-user.target

